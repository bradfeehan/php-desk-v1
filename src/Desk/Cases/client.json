{
	"name": "Cases",
	"description": "Client for Desk case API",
	"operations": {
		"GetCases": {
			"summary": "Search cases based on a combination of parameters with pagination",
			"httpMethod": "GET",
			"uri": "cases.json",
			"responseType": "class",
			"responseClass": "Desk\\Cases\\Model\\CaseArray",
			"parameters": {
				"name":             {"location": "query"},
				"first_name":       {"location": "query"},
				"last_name":        {"location": "query"},
				"email":            {"location": "query"},
				"phone":            {"location": "query"},
				"company":          {"location": "query"},
				"twitter":          {"location": "query"},
				"labels":           {"location": "query"},
				"case_id":          {"location": "query"},
				"subject":          {"location": "query"},
				"description":      {"location": "query"},
				"status":           {"location": "query"},
				"priority":         {"location": "query"},
				"assigned_group":   {"location": "query"},
				"assigned_user":    {"location": "query"},
				"channels":         {"location": "query"},
				"notes":            {"location": "query"},
				"attachments":      {"location": "query"},
				"created":          {"location": "query"},
				"updated":          {"location": "query"},
				"since_created_at": {"location": "query"},
				"max_created_at":   {"location": "query"},
				"since_updated_at": {"location": "query"},
